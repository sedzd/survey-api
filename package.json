{
    "name": "survey",
    "version": "1.0.0",
    "description": "Survey API",
    "repository": {
        "type": "git"
    },
    "main": "index.js",
    "engines": {
        "node": ">=16.6.2",
        "npm": ">=7.20.3"
    },
    "dependencies": {
        "@drdgvhbh/postgres-error-codes": "^0.0.6",
        "compression": "^1.7.4",
        "date-fns": "^2.29.3",
        "env-var": "^7.3.0",
        "express": "^4.18.2",
        "express-http-context": "^1.2.4",
        "helmet": "^6.0.1",
        "http-graceful-shutdown": "^3.1.12",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "pg": "^8.8.0",
        "pg-protocol": "^1.5.0",
        "pino": "^8.11.0",
        "pino-http": "^8.3.3",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.21",
        "typeorm": "^0.3.11",
        "yup": "^1.0.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/errorhandler": "1.5.0",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.2.5",
        "@types/lodash": "^4.14.191",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^18.11.18",
        "@types/pg": "^8.6.6",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.31.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.2.0",
        "eslint-plugin-jest-extended": "^2.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^29.3.1",
        "jest-extended": "^3.2.1",
        "lefthook": "^1.2.6",
        "nodemon": "^2.0.20",
        "pino-pretty": "^9.1.1",
        "prettier": "2.8.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4"
    },
    "scripts": {
        "start": "node ./dist/src/server.js",
        "start:dev": "nodemon -r dotenv/config ./src/server.ts",
        "build": "rm -rf dist && tsc",
        "format:check": "prettier --config .prettierrc.js --check './**/*.{js,ts,json,yml,yaml}'",
        "format": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}'",
        "lint:fix": "eslint --ignore-path .eslintignore --ext .ts,.js --fix .",
        "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
        "migration:create": "npm run typeorm -- migration:create src/libs/typeorm/migrations/$npm_config_name",
        "migration:generate": "npm run typeorm migration:generate -- -d ./src/libs/typeorm/ormconfig-cli.ts src/libs/typeorm/migrations/$npm_config_name",
        "migration:revert": "npm run typeorm migration:revert -- -d ./src/libs/typeorm/ormconfig-cli.ts",
        "migration:run": "npm run typeorm migration:run -- -d ./src/libs/typeorm/ormconfig-cli.ts",
        "test:all": "cross-env jest --verbose --runInBand --collectCoverage=true --detectOpenHandles ./tests",
        "test:watch": "jest -i --watch --verbose",
        "test": "jest",
        "typeorm": "ts-node -r ./src/module-alias.ts -r dotenv/config ./node_modules/typeorm/cli"
    },
    "author": "saeed",
    "license": "UNLICENSED",
    "private": true
}
